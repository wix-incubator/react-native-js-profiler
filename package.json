{
  "name": "react-native-js-profiler",
  "version": "0.2.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "release": "node ./scripts/release.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wix-incubator/react-native-js-profiler.git"
  },
  "keywords": [
    "react-native",
    "profiler",
    "devtools",
    "performance"
  ],
  "author": "Andy Kogut <mail@andykog.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wix-incubator/react-native-js-profiler/issues"
  },
  "homepage": "https://github.com/wix-incubator/react-native-js-profiler#readme",
  "peerDependencies": {
    "react-native": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "babel-preset-react-native": "^4.0.1",
    "jest": "^23.6.0",
    "react": "^16.6.3",
    "react-native": "^0.57.x",
    "semver": "5.x.x",
    "shell-utils": "1.x.x",
    "lodash": ">=4.17.5"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "react-native"
        ],
        "retainLines": true
      }
    }
  },
  "jest": {
    "transformIgnorePatterns": [],
    "preset": "react-native",
    "roots": [
      "node_modules",
      "src"
    ],
    "resetMocks": true,
    "resetModules": true,
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  },
  "dependencies": {
    "detox-instruments-react-native-utils": "3.x.x"
  }
}
